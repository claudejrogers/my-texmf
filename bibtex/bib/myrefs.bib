	Date-Added = {2009-06-17 02:47:58 -0700},
	Date-Modified = {2009-06-17 02:50:52 -0700},
	Journal = {J. Biol. Chem.},
	Keywords = {CS; NTs},
	Number = {7},
	Pages = {4110-4117},
	Title = {Complex carbohydrates of cultured {PC12} pheochromocytoma cells},
