	Journal = {Nat. Chem. Biol.},
	Keywords = {CS; CS antibodies: CS neuronal growth; microarrays},
	Number = {9},
	Pages = {467-473},
	Title = {Sulfation patterns of glycosaminoglycans encode molecular recognition and activity},
