	Keywords = {CS},
	Number = {27},
	Pages = {20793--20805},
	Pmid = {20439988},
	Title = {Mice deficient in \emph{N}-acetylgalactosamine 4-sulfate 6-\emph{O}-sulfotransferase are unable to synthesize chondroitin/dermatan sulfate containing \emph{N}-acetylgalactosamine 4,6-bissulfate residues and exhibit decreased protease activity in bone marrow-derived mast cells},
