	Journal = {Neuron},
	Keywords = {CS; HS; retinal development; CS guidance; stripe assay},
	Pages = {961--965},
	Title = {Semaphorin 5{A} is a bifunctional axon guidance cue regulated by heparan and chondroitin sulfate proteoglycans},
	Volume = {44},
	Year = {2005}}

