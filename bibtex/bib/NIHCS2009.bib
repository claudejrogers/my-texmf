@article{bao05,
	Abstract = {Mammalian brains contain significant amts. of chondroitin sulfate (CS), dermatan sulfate (DS), and CS/DS hybrid chains. CS/DS chains isolated from embryonic pig brains (E-CS/DS) promote the outgrowth of neurites in embryonic mouse hippocampal neurons in culture by interacting with pleiotrophin (PTN), a heparin-binding growth factor. Here, we analyzed oligosaccharides isolated from E-CS/DS, which showed that octasaccharides were the minimal size capable of interacting with PTN at a physiol. salt concn. Five and eight sequences were purified from fluorescently labeled PTN-bound and -unbound octasaccharide fractions, resp., by enzymic digestion followed by PTN-affinity chromatog. Their sequences were detd. by enzymic digestion in conjunction with high performance liq. chromatog., revealing a crit. role for oversulfated D and/or iD disaccharides in the low yet significant affinity for PTN, which is required for neuritogenesis. The crit. D and iD units are GlcUA(2-O-sulfate)b1-3GalNAc(6-O-sulfate) and IdoUA(2-O-sulfate)a1-3GalNAc(6-O-sulfate), resp., where IdoUA represents L-iduronic acid. In contrast, high affinity interactions with PTN required decasaccharides with E units (GlcUAb1-3GalNAc(4, 6-O-disulfate)), B units (GlcUA(2-O-sulfate)b1-3GalNAc(4-O-sulfate)), and/or their IdoUA-contg. counterparts (iE and iB) in addn. to D/iD units, although the biol. significance of such strong interactions remains to be investigated. Thus, chain size and compn. are crucial to the interaction with PTN, and PTN binds to multiple sequences in E-CS/DS chains with distinct affinity. Notably, not only heparan sulfate but also CS/DS hybrid chain structures of mammalian brains contain a high degree of microheterogeneity with a cluster of oversulfated disaccharides and appear to play roles in regulating the functions of PTN. [on SciFinder (R)]},
	Author = {Bao, X. and Muramatsu, T. and Sugahara, K.},
	Date-Modified = {2009-08-03 20:44:16 -0700},
	Journal = {J. Biol. Chem.},
	Number = {42},
	Pages = {35318-35328},
	Title = {Demonstration of the pleiotrophin-binding oligosaccharide sequences isolated from chondroitin sulfate/dermatan sulfate hybrid chains of embryonic pig brains},
	Volume = {280},
	Year = {2005}}

