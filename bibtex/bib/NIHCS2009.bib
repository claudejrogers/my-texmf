@article{Galtrey07,
	Abstract = {A review. Chondroitin sulfate proteoglycans (CSPGs) consist of a core protein and glycosaminoglycan (GAG) chains. There is enormous structural diversity among CSPGs due to variation in the core protein, the no. of GAG chains and the extent and position of sulfation. Most CSPGs are secreted from cells and participate in the formation of the extracellular matrix (ECM). CSPGs are able to interact with various growth-active mols. and this may be important in their mechanism of action. In the normal central nervous system (CNS), CSPGs have a role in development and plasticity during postnatal development and in the adult. Plasticity is greatest in the young, esp. during crit. periods. CSPGs are crucial components of perineuronal nets (PNNs). PNNs have a role in closure of the crit. period and digestion of PNNs allows their re-opening. In the adult, CSPGs play a part in learning and memory and the hypothalamo-neurohypophysial system. CSPGs have an important role in CNS injuries and diseases. After CNS injury, CSPGs are the major inhibitory component of the glial scar. Removal of CSPGs improves axonal regeneration and functional recovery. CSPGs may also be involved in the pathol. processes in diseases such as epilepsy, stroke and Alzheimer's disease. Several possible methods of manipulating CSPGs in the CNS have recently been identified. The development of methods to remove CSPGs has considerable therapeutic potential in a no. of CNS disorders. [on SciFinder (R)]},
	Author = {Galtrey, C. M. and Fawcett, J. W.},
	Date-Modified = {2009-08-03 20:45:54 -0700},
	Journal = {Brain Res. Rev.},
	Number = {1},
	Pages = {1-18},
	Title = {The role of chondroitin sulfate proteoglycans in regeneration and plasticity in the central nervous system},
	Volume = {54},
	Year = {2007}}

